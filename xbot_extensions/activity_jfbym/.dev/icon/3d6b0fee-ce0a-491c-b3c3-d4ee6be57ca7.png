<script>
        function getQueryVariable(variable)
    {
        var query = window.location.search.substring(1);
        var vars = query.split("&");
        for (var i=0;i<vars.length;i++) {
            var pair = vars[i].split("=");
            if(pair[0] == variable){return pair[1];}
        }
        return(false);
    }

        function checkHash(hash, istb) {
        var defaultHash = istb == "true" ? "#taobao" : "#cainiao";
        var allowList = ['#taobao', '#cainiao', '#dingding'];
        if (allowList.includes(hash)) {
            return hash
        } else {
            return defaultHash;
        }
    }

        function loadDingDingScript() {
            }

        function onHashChange() {
        var istb = getQueryVariable("istb");
        tbHash = istb == "true" ? "#taobao" : "";
        var hash = location.hash || tbHash || '#cainiao';
                hash = checkHash(hash, istb)
        $('.login-frames a[href~="' + hash + '"]')
                .addClass('active').siblings().removeClass('active');

        $('#J_' + hash.substr(1)).addClass('active').siblings().removeClass('active');

                loadDingDingScript()
    }
    $(window).on('hashchange', onHashChange);
    onHashChange();

        var miniLoginEmbedder = new window.MiniLoginEmbedder();
         //监听登录完成后的消息，resize已被监听
     miniLoginEmbedder.addEvent('onMessage', function (args) {
        //hasLoginResult:免登成功事件;loginResult:登录成功事件
        if (args.action && (args.action == "hasLoginResult" || args.action == "loginResult")) {
            if (args.st) {   // stSite为 ValidateSt 的时候传的site参数
                location.href = "https://cnlogin.cainiao.com/doLogin?stSite=" + args.stSite + "&st=" + args.st + "&loginChannel=pc" + "&account=" + encodeURIComponent(args.loginId)
                        + "&redirectURL=http%3A%2F%2Fcnlogin.cainiao.com%2Ffavicon.ico";
            }
        }
    });
    
    miniLoginEmbedder.init({
        targetId: 'alibaba-login-iframe',
        appName: 'cainiao',   // 需要自己申请，这个只是用来展示测试的
        iframeUrl:'https://passport.cainiao.com/mini_login.htm',
        // iframeUrl: 'https://passport.cainiao.com/mini_login.htm',
